#include<iostream>
#include<string>
using namespace std;
class Jeans{
    public:
    string type[3]={"bootcut","wide leg","skinny"};
    void brand(){
        cout<<"H&M -&DENIM"<<endl;
        
    }
};
class bootcut:public Jeans{
    public:
    string color="Dark blue";
    
};
class wl:public Jeans{
    public:
    string color="black";
};
class skinny:public Jeans{
    public:
    string color ="grey";
    
};

int main()
{
bootcut a;
cout<<endl;
a.brand();
cout<<a.type[0]<<" ."<<a.color<<endl;
wl b;
cout<<endl;
b.brand();
cout<<b.type[1]<<";"<<b.color<<endl;

   return 0;
    
}
